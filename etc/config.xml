<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <content_generator>
            <general>
                <enabled>0</enabled>
                <stores>0</stores>
                <notification_template>content_generator_general_notification_template</notification_template>
            </general>
            <api>
                <end_point_url>https://api.openai.com</end_point_url>
                <token backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <openai_model>gpt-3.5-turbo-instruct</openai_model>
                <temperature>0.5</temperature>
                <top_p>0</top_p>
                <frequency_penalty>0</frequency_penalty>
                <presence_penalty>0</presence_penalty>
            </api>
        </content_generator>
    </default>
</config>
